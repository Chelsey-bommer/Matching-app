<!DOCTYPE html>
<html lang="en">

<head>
  <%- include('../partials/head') %>
    <title>Login</title>
</head>


<body>
  <figure>
    <img
      src="https://images.unsplash.com/photo-1488707872600-5507977fe355?ixlib=rb-1.2.1&ixid=MnwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8&auto=format&fit=crop&w=1770&q=80"
      alt="">
  </figure>

  <section>
    <form action="/login" method="post" enctype="multipart/form-data">
      <legend>Login</legend>
      <div>
        <label for="email">E-mail</label>
        <input type="email" id="email" name="email" placeholder="voorbeeld@mail.com" />
      </div>
      <div>
        <label for="password">Password</label>
        <input type="password" id="password" name="password" />
      </div>
      <button type="submit">Login</button>
    </form>
  </section>


  <!-- Link javascript -->
  <script src="/static/js/script.js"></script>

</body>

</html>